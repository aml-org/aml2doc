<!DOCTYPE html>
<html lang="en">
  <head>
    <title>{{name}}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/custom.css">
  </head>
  <body>
    <div class="container-fluid">
      <div class="col-md-2 aside">
        <div>
          <h1>Dialects</h1>
          <hr>
          {{#navData.dialects}}
            <p><a href="{{htmlName}}" title="{{name}}">{{name}}</a></p>
          {{/navData.dialects}}
        </div>
        <div>
          <h1>Schemas</h1>
          <hr>
          {{#navData.nodeMappings}}
            <p><a href="{{htmlName}}" title="{{name}}">{{name}}</a></p>
          {{/navData.nodeMappings}}
        </div>
      </div>
      <div class="col-md-10">
        <h1>{{name}}</h1>
        <p>URL: {{id}}</p>
        <p>Schemas:</p>
        <ul>
          {{#nodeMappings}}
            <li><a href="{{htmlName}}" title="{{name}}">{{name}}</a></li>
          {{/nodeMappings}}
        </ul>
      </div>
    </div>
  </body>
</html>
